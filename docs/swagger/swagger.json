                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Tracks"
                ],
                "summary": "Retrive track",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "track id",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
