                        "type": "string",
                        "description": "Limit result.",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Offset result.",
                        "name": "offset",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Name of the artist or group.",
                        "name": "artist",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Title of track.",
                        "name": "track",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "List of tracks.",
                        "name": "releasedyaer",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Exact link",
                        "name": "link",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success response",
